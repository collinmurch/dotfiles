{
    "editor.fontSize": 15,
    "terminal.integrated.fontSize": 15,
    "editor.tabSize": 4,
    "editor.fontLigatures": true,
    "workbench.startupEditor": "newUntitledFile",
    "explorer.confirmDelete": false,
    "editor.minimap.enabled": false,
    "yaml.customTags": [
        "!reference sequence"
    ],
    "explorer.confirmDragAndDrop": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.scrollbar.vertical": "auto",
    "eslint.validate": [
        "javascript"
    ],
    "editor.suggestSelection": "first",
    "terminal.integrated.cursorStyle": "underline",
    "git.confirmSync": false,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontFamily": "TX-02",
    "go.toolsManagement.autoUpdate": true,
    "redhat.telemetry.enabled": false,
    "window.commandCenter": false,
    "editor.accessibilitySupport": "off",
    "git.ignoreRebaseWarning": true,
    "workbench.sideBar.location": "right",
    "workbench.layoutControl.enabled": false,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "extensions.ignoreRecommendations": true,
    "security.workspace.trust.untrustedFiles": "newWindow",
    "git.autofetch": false,
    "git.openRepositoryInParentFolders": "always",
    "gitlens.views.repositories.autoRefresh": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "codeium.enableCodeLens": false,
    "go.editorContextMenuCommands": {
        "playground": false,
        "debugTestAtCursor": false,
        "testCoverage": false,
        "addImport": false,
        "generateTestForFunction": false,
        "testAtCursor": false,
        "addTags": false,
        "toggleTestFile": false
    },
    "gopls": {
        "ui.codelenses": {
            "generate": false,
            "gc_details": false,
            "regenerate_cgo": false,
            "run_govulncheck": false,
            "tidy": false,
            "upgrade_dependency": false,
            "vendor": false,
            "test": true
        },
        "build.buildFlags": [
            "-tags=integration"
        ],
        "build.standaloneTags": [
            "wireinject",
            "!wireinject",
            "tools"
        ]
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "codeium.enableCommandSideHintEducation": false,
    "codeium.disableSupercomplete": true,
    "go.survey.prompt": false,
    "go.showWelcome": false,
    "yaml.format.singleQuote": true,
    "workbench.colorTheme": "poimandres"
}